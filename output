response = None
error = NewConnectionError('<urllib3.connection.HTTPSConnection object at 0x108e76830>: Failed to establish a new connection: [Errno 8] nodename nor servname provided, or not known')
_pool = <urllib3.connectionpool.HTTPSConnectionPool object at 0x108e778e0>
